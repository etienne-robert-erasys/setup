# Aliases
alias ls="ls -F"
alias mv="mv -i"
alias cp="cp -i"
alias getmake="cp ~/tools/ctools/Makefile ."
alias getmain="cp ~/tools/ctools/main.c ."
alias getctools="getmake ; getmain"
alias getcpptools="cp ~/tools/cpptools/Makefile ~/tools/cpptools/main.cpp ."
alias LS="ls"
alias sl="ls"
alias SL="ls"
alias rm="rm -v"
alias make="make -j8"

# git aliases
alias gs="git status"
alias gc="git commit"
alias ga="git add"
alias gp="git push"
alias gpl="git pull"
alias gd="git diff"

